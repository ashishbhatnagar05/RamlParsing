#%RAML 1.0
title: Amazon S3 REST API
version: 1
baseUri: https://{bucketName}.s3.amazonaws.com
(sensitive):
baseUriParameters:
  bucketName:
    description: The name of the bucket

annotationTypes:
  sensitive:
    type: nil

types:
  User:
    type: object
    properties:
      firstname: string
      lastname:  string
      age:
        type: number
        (sensitive):

/users/{id}:
  get:
    responses:
      200:
        body:
          application/json:
            type: User
